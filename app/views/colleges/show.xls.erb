<table border = 7>
  <thead>
    <tr>
      <th>Sl.No</th>
      <th>Name</th>
      <th>Email</th>
      <th>Mobile</th>
      <th>College</th>
      <th>Beneficial</th>
      <th>Join</th>
      <th>Consultant</th>
      <th>Suggestion</th>

      <!-- <th colspan="3"></th> -->
    </tr>
  </thead>

  <tbody>
    <% @college.students.each_with_index do |student,index| %>
      <tr>
        <td><%= index + 1 %></td>
        <td><%= student.name %></td>
        <td><%= student.email %></td>
        <td><%= student.mobile %></td>
        <td><%= student.college.name %></td>
        <td><%= student.beneficial %></td>
        <td><%= student.joining %></td>
        <td><%= student.consult %></td>
        <td><%= student.suggestion %></td>

        <!-- <td><%= link_to 'Show', student %></td> -->

      </tr>
    <% end %>
  </tbody>
</table>
