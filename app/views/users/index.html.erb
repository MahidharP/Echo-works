<h2>Listing Users</h2>

<table>
    <thead>
        <tr>
            <th>Email</th>
        </tr>
    </thead>

    <tbody>
        <% @users.each do |user| %>
        <tr>
            <td><%= user.email %></td>
        </tr>
        <% end %>
    </tbody>
</table>

<%= link_to "New User", new_user_path %>
