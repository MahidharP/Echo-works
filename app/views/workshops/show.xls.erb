<table border = 7>
  <thead>
    <tr>
      <th>Name</th>
      <th>Email</th>
      <th>Mobile</th>
      <th>College code</th>
      <th>Beneficial</th>
      <th>Feedback</th>
      <th>Join</th>
      <th>Consultant</th>
      <th>Technology</th>
      <th>Suggestion</th>

    </tr>
  </thead>

  <tbody>
    <% @workshop.students.each do |student| %>
      <tr>
        <td><%= student.name %></td>
        <td><%= student.email %></td>
        <td><%= student.mobile %></td>
        <td><%= student.college.name %></td>
        <td><%= student.beneficial %></td>
        <td><%= student.feedback %></td>
        <td><%= student.joining %></td>
        <td><%= student.consult %></td>
        <td><% student.technologies.each do |technology| %>
          <%= technology.name %>
          <% end %>
        <td><%= student.suggestion %></td>

      </tr>
    <% end %>
  </tbody>
</table>
